{
  "name": "react-frontentd-lib",
  "version": "1.0.0",
  "description": "React frontend library",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": ">=8.5.0",
    "yarn": ">=1.22.18",
    "node": ">=16.14.2"
  },
  "homepage": "https://gitlab.com/epifanovmd/react-frontend-lib",
  "author": {
    "name": "Epifanov Andrei",
    "email": "epifanovmd@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:epifanovmd/react-frontend-lib.git"
  },
  "scripts": {
    "build": "webpack",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*{.ts,.tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*{.ts,.tsx}\"",
    "prettier:fix": "prettier --parser typescript --write src/**/*.{ts,tsx}",
    "check-packages-updates": "yarn outdated"
  },
  "dependencies": {
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "inversify-react": "^1.0.2",
    "lodash": "^4.17.21",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.16",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.190",
    "@types/react": "^18.0.25",
    "@types/shortid": "^0.0.29",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^4.3.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "react-dev-utils": "^12.0.1",
    "react-hooks": "^1.0.1",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix \"src/**/*{.ts,.tsx}\"",
      "prettier --parser typescript --write src/**/*.{ts,tsx}",
      "git add"
    ]
  }
}
